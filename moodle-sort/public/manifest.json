{
    "manifest_version": 3,
    "name": "Moodle Sorter",
    "description": "Base Level Extension",
    "version": "1.0.0",
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "scripting"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*",
        "localhost:*"
    ]
}